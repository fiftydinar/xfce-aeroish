[Unit]
Description=Fix groups during rebase
DefaultDependencies=no
Before=systemd-sysusers.service
After=systemd-remount-fs.service
ConditionPathExists=!/etc/arch-group-rebase-fix.stamp

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=/usr/bin/arch-group-rebase-fix

[Install]
WantedBy=sysinit.target
